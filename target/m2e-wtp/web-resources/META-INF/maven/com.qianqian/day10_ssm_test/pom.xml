	    <artifactId>tomcat7-maven-plugin</artifactId>
	    <version>2.2</version>
	    <configuration>
	    <url>http://127.0.0.1:8080/manager/text</url>
	    <port>8888</port>
	    <uriEncoding>utf-8</uriEncoding>
	    <path>/ssm_demo</path>
	    </configuration>
	    </plugin>
	</plugins>
